<div class="head">
  <div class="user_box">
    <div>
      <%= @user.name %> -
    </div>
    <div>
      # <%= @user.steam_id %>
    </div>
  </div>

  <div class="header">
    <div>Game Title</div>
    <div>Hours spent</div>
  </div>
</div>

<div class="game_box">
  <% @test.each do |game| %>
      <div>
        <div>
          <%= game.game.title %>
        </div>

        <div>
          <%= game.hours %>
        </div>
      </div>
  <% end %>
</div>
